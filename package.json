{
  "name": "cfd",
  "version": "4.0.1",
  "description": "Starter project for React, TypeScript and StoryBook",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "https://github.com/aslakhellesoy/cfd",
  "author": "Aslak Helles√∏y",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "nyc": "nyc --reporter=html --reporter=text mocha",
    "test": "mocha",
    "compile": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "eslint": "eslint --ext ts,tsx --max-warnings 0 src test",
    "eslint-fix": "eslint --ext ts,tsx --max-warnings 0 --fix src test"
  },
  "dependencies": {
    "d3": "^6.7.0",
    "d3-axis": "^3.0.0",
    "d3-scale": "^4.0.2",
    "next": "15.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@cucumber/microdata": "^1.3.0",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-controls": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/nextjs": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@types/d3": "^7.4.3",
    "@types/jsdom": "^16.2.5",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.9",
    "@types/papaparse": "^5.2.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.1",
    "csv-loader": "^3.0.5",
    "eslint": "^9",
    "husky": "^4.3.0",
    "jsdom": "^16.4.0",
    "mocha": "^11.0.1",
    "nyc": "^17.1.0",
    "papaparse": "^5.4.1",
    "postcss": "^8",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "react-is": "^17.0.1",
    "schema-dts": "^0.7.1",
    "source-map-support": "^0.5.21",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.4.1",
    "ts-loader": "8.0.11",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage",
      "stories"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  }
}
